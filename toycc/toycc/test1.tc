int add(int a, int b) {
    return a + b;
}

int main() {
    int sum = 0;
    int i = 0;
    while (i < 10) {
        if (i == 5) {
            i = i + 1;
            continue;
        }
        if (i == 8) {
            break;
        }
        sum = sum + add(i, 1);
        i = i + 1;
    }
    return sum;
}
